<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Storyboard x:Key="MoveBack">
        <DoubleAnimation Storyboard.TargetName="Translate" Storyboard.TargetProperty="Y" To="0" Duration="0:0:0.40" AccelerationRatio="0.3" DecelerationRatio="0.7" />
        <DoubleAnimation Storyboard.TargetName="Translate" Storyboard.TargetProperty="Y" BeginTime="0:0:0.40" To="-50" Duration="0:0:0.40" AccelerationRatio="0.6" DecelerationRatio="0.4" />
        <DoubleAnimation Storyboard.TargetName="Translate" Storyboard.TargetProperty="Y" BeginTime="0:0:0.80" To="0" Duration="0:0:0.40" AccelerationRatio="0.2" DecelerationRatio="0.8" />
    </Storyboard>
    <Storyboard x:Key="MoveUp">
        <DoubleAnimation Storyboard.TargetName="Translate" Storyboard.TargetProperty="Y" Duration="0:0:1.00" AccelerationRatio="0.4" DecelerationRatio="0.6" />
    </Storyboard>
</ResourceDictionary>